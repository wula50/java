package study;

import java.lang.reflect.Array;
import java.util.Arrays;

public class Ex6_15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char[] arr1= {'a','b','c','d','e'};
		char[] arr2=Arrays.copyOfRange(arr1, 0, 3);
		for(char c:arr1) {
			System.out.print(c+" ");
		}
		for(char c:arr2) {
			System.out.print(c+" ");
		}
		

	}

}
