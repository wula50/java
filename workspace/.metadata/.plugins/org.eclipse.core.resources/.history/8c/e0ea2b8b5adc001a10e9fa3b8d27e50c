package study;

public class Ex5_20 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str1="abcdefg";
		String str2="aBcdddd";
		boolean b=false;
		// 从第一个参数到第二个参数，包含第二个参数
		b=str1.substring(0, 5).equalsIgnoreCase(str2.substring(0,5));
		if(b) {
			System.out.println("两个子串相同");
		}else {
			System.out.println("两个子串不相同");
		}
		
		

	}

}
