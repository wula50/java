package study;

public class Ex7_06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Test test=new Test();
		test.setName(100);
		int n=test.getName();
		System.out.println(n);

	}

}


class Test{
	private int name;

	public int getName() {
		return name;
	}

	public void setName(int name) {
		this.name = name;
	}
	
	
}
