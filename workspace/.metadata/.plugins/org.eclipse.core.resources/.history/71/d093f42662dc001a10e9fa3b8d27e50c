package study;

public class Ex6_16 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr1= {1,79,48,9,45,8};
		int min=Integer.MIN_VALUE;
		for(int i=0;i<arr1.length;i++) {
			if(min>arr1[i]) {
				min=arr1[i];
			}
		}
		System.out.println("数组中最小的值是："+min);

	}

}
